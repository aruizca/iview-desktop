<?xml version='1.0' encoding='utf-8'?>
<application xmlns="http://ns.adobe.com/air/application/3.7">
	<id>iViewDesktop</id>
	<name>iView - Desktop</name>
	<filename>iView-Desktop</filename>
	<versionNumber>1.0.0</versionNumber>
	<installFolder>iView-Desktop</installFolder>
	<initialWindow>
		<title>iView - Desktop</title>
		<content>redirect.html</content>
		<systemChrome>standard</systemChrome>
		<transparent>false</transparent>
		<visible>true</visible>
		<resizable>true</resizable> 
		<renderMode>direct</renderMode>
		<aspectRatio>landscape</aspectRatio> 
		<autoOrients>true</autoOrients>
		<width>1024</width>	
		<height>768</height>	
	</initialWindow>
    <icon>
        <image16x16>icons/iview16.png</image16x16>
        <image32x32>icons/iview32.png</image32x32>
        <image48x48>icons/iview48.png</image48x48>
        <image128x128>icons/iview128.png</image128x128>
    </icon>
</application>